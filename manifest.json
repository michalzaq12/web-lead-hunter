{
    "name": "Web Lead Hunter",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs", "scripting", "storage"
    ],
    "host_permissions": [
        "http://*/*", "https://*/*"
    ],
    "action": {
        "default_icon": "hello-16.png",
        "default_popup": "popup.html"
    }
}